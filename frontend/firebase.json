{
  "hosting": [
    {
      "target": "frontend",
      "public": "out",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
      "cleanUrls": true,
      "rewrites":  [
      { "source": "/auth", "destination": "/auth.html" },
      { "source": "/console", "destination": "/console.html" },
      { "source": "/editor", "destination": "/editor.html" },
      { "source": "/", "destination": "/index.html" }
    ]
    }           
  ]             
}